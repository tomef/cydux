{
  "name": "cydux",
  "version": "0.1.1",
  "description": "Event handler and data store for React.js projects",
  "main": "lib/index.js",
  "scripts": {
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/cydux.js --config webpack.config.development.js",
    "build:umd:min": "webpack -p src/index.js dist/cydux.min.js --config webpack.config.production.js",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [
    "reactive",
    "events",
    "react",
    "state"
  ],
  "author": "Thomas Feagin",
  "license": "MIT",
  "dependencies": {
    "eventemitter3": "^1.1.1",
    "most": "^0.17.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.3",
    "immutable": "^3.7.6",
    "jsdom": "^8.0.1",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "webpack": "^1.12.12"
  }
}
